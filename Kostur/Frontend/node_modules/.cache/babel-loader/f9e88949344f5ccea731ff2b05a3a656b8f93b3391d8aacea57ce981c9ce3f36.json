'',\r\n        factoryId: '', // factory ID\r\n        picture: ''\r\n      },\r\n      chocolateTypes: ['PLAIN', 'FOR_COOKING', 'FOR_DRINKING'],\r\n      chocolateKinds: ['DARK', 'MILK', 'WHITE']\r\n    };\r\n  },\r\n  created() {\r\n    this.chocolate.factoryId = this.$route.params.factoryId; // Postavi ID fabrike iz parametara rute\r\n  },\r\n  methods: {\r\n    async addChocolate() {\r\n      try {\r\n        // Postavljanje početnog stanja\r\n        this.chocolate.quantity = 0;\r\n        this.chocolate.status = 'OUT_OF_STOCK';\r\n\r\n        // Slanje podataka o čokoladi na server\r\n        await axios.post('http://localhost:8080/WebShopAppREST/rest/chocolates', this.chocolate);\r\n        \r\n        // Preusmeravanje na stranicu sa svim čokoladama\r\n        this.$router.push({ name: 'allchocolates' });\r\n      } catch (error) {\r\n        console.error('Greška pri dodavanju čokolade:', error);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Stilovi za komponentu */\r\n.chocolate-form {\r\n  max-width: 400px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 15px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  font-weight: bold;\r\n}\r\n\r\ninput[type=\"text\"],\r\ninput[type=\"number\"],\r\nselect,\r\ntextarea {\r\n  width: 100%;\r\n  padding: 8px;\r\n  box-sizing: border-box;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n}\r\n\r\ntextarea {\r\n  height: 100px;\r\n}\r\n\r\nbutton {\r\n  width: 100%;\r\n  padding: 10px;\r\n  background-color: #007bff;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n</style>\r\n"],"mappings":";AA2CA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,SAAS,EAAEC,QAAO,QAAS,YAAY;AAEhD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;QACTC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,CAAC;QACRC,IAAI,EAAE,EAAE;QACRC,IAAI,EAAE,EAA