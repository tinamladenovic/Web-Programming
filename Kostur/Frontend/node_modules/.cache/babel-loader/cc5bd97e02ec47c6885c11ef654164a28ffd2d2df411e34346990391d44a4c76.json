{"ast":null,"code":"import axios from 'axios';\nimport { ref, onMounted, computed } from 'vue';\nexport default {\n  setup() {\n    const users = ref([]);\n    const sortBy = ref('firstName'); // Default sort by first name ascending\n    const roleFilter = ref(''); // Default no role filter\n\n    const fetchUsers = async () => {\n      try {\n        const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/users', {\n          withCredentials: true\n        });\n        users.value = response.data;\n        console.log('Fetched users:', users.value); // Log after successful fetch\n      } catch (error) {\n        console.error('Error fetching users:', error);\n      }\n    };\n    onMounted(() => {\n      fetchUsers();\n    });\n    const filteredUsers = computed(() => {\n      let filtered = users.value;\n\n      // Filter by role if roleFilter is set\n      if (roleFilter.value) {\n        filtered = filtered.filter(user => user.role === roleFilter.value);\n      }\n\n      // Sort users based on selected sortBy value\n      if (sortBy.value.startsWith('-')) {\n        const key = sortBy.value.substring(1);\n        filtered.sort((a, b) => b[key].localeCompare(a[key])); // Descending sort\n      } else {\n        filtered.sort((a, b) => a[sortBy.value].localeCompare(b[sortBy.value])); // Ascending sort\n      }\n      return filtered;\n    });\n    return {\n      filteredUsers,\n      sortBy,\n      roleFilter\n    };\n  }\n};","map":{"version":3,"names":["axios","ref","onMounted","computed","setup","users","sortBy","roleFilter","fetchUsers","response","get","withCredentials","value","data","console","log","error","filteredUsers","filtered","filter","user","role","startsWith","key","substring","sort","a","b","localeCompare"],"sources":["C:\\Users\\PC\\eclipse-workspace\\apache-tomcat-9.0.89\\apache-tomcat-9.0.89\\webapps\\chocolate-factory-2\\Kostur\\Frontend\\front-app\\src\\views\\UserTable.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Registered Users</h2>\r\n\r\n    <!-- Sorting controls -->\r\n    <div>\r\n      <label>Sort by:</label>\r\n      <select v-model=\"sortBy\">\r\n        <option value=\"firstName\">First Name (A-Z)</option>\r\n        <option value=\"-firstName\">First Name (Z-A)</option>\r\n        <option value=\"lastName\">Last Name (A-Z)</option>\r\n        <option value=\"-lastName\">Last Name (Z-A)</option>\r\n        <option value=\"username\">Username (A-Z)</option>\r\n        <option value=\"-username\">Username (Z-A)</option>\r\n        <option value=\"earnedPoints\">Earned Points (Ascending)</option>\r\n        <option value=\"-earnedPoints\">Earned Points (Descending)</option>\r\n      </select>\r\n    </div>\r\n\r\n    <!-- Filtering controls -->\r\n    <div>\r\n      <label>Filter by role:</label>\r\n      <select v-model=\"roleFilter\">\r\n        <option value=\"\">All</option>\r\n        <option value=\"ADMIN\">Admin</option>\r\n        <option value=\"CUSTOMER\">Customer</option>\r\n        <option value=\"WORKER\">Worker</option>\r\n        <option value=\"MANAGER\">Manager</option>\r\n      </select>\r\n    </div>\r\n\r\n    <table class=\"table\">\r\n      <thead>\r\n        <tr>\r\n          <th>First Name</th>\r\n          <th>Last Name</th>\r\n          <th>Username</th>\r\n          <th>Earned Points</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"user in filteredUsers\" :key=\"user.id\">\r\n          <td>{{ user.firstName }}</td>\r\n          <td>{{ user.lastName }}</td>\r\n          <td>{{ user.username }}</td>\r\n          <td>{{ user.earnedPoints }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { ref, onMounted, computed } from 'vue';\r\n\r\nexport default {\r\n  setup() {\r\n    const users = ref([]);\r\n    const sortBy = ref('firstName'); // Default sort by first name ascending\r\n    const roleFilter = ref(''); // Default no role filter\r\n\r\n    const fetchUsers = async () => {\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/users', {\r\n          withCredentials: true,\r\n        });\r\n        users.value = response.data;\r\n        console.log('Fetched users:', users.value); // Log after successful fetch\r\n      } catch (error) {\r\n        console.error('Error fetching users:', error);\r\n      }\r\n    };\r\n\r\n    onMounted(() => {\r\n      fetchUsers();\r\n    });\r\n\r\n    const filteredUsers = computed(() => {\r\n      let filtered = users.value;\r\n\r\n      // Filter by role if roleFilter is set\r\n      if (roleFilter.value) {\r\n        filtered = filtered.filter(user => user.role === roleFilter.value);\r\n      }\r\n\r\n      // Sort users based on selected sortBy value\r\n      if (sortBy.value.startsWith('-')) {\r\n        const key = sortBy.value.substring(1);\r\n        filtered.sort((a, b) => b[key].localeCompare(a[key])); // Descending sort\r\n      } else {\r\n        filtered.sort((a, b) => a[sortBy.value].localeCompare(b[sortBy.value])); // Ascending sort\r\n      }\r\n\r\n      return filtered;\r\n    });\r\n\r\n    return {\r\n      filteredUsers,\r\n      sortBy,\r\n      roleFilter,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\n\r\n.table th, .table td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n}\r\n\r\n.table th {\r\n  background-color: #f2f2f2;\r\n  text-align: left;\r\n}\r\n</style>\r\n"],"mappings":"AAqDA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,GAAG,EAAEC,SAAS,EAAEC,QAAO,QAAS,KAAK;AAE9C,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIJ,GAAG,CAAC,EAAE,CAAC;IACrB,MAAMK,MAAK,GAAIL,GAAG,CAAC,WAAW,CAAC,EAAE;IACjC,MAAMM,UAAS,GAAIN,GAAG,CAAC,EAAE,CAAC,EAAE;;IAE5B,MAAMO,UAAS,GAAI,MAAAA,CAAA,KAAY;MAC7B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMT,KAAK,CAACU,GAAG,CAAC,iDAAiD,EAAE;UAClFC,eAAe,EAAE;QACnB,CAAC,CAAC;QACFN,KAAK,CAACO,KAA