{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      newUser: {\n        firstName: '',\n        lastName: '',\n        username: '',\n        password: '',\n        gender: 'MALE',\n        birthDate: '',\n        role: 'MANAGER',\n        customerType: 'NO_TYPE',\n        earnedPoints: 0,\n        factoryId: '' // Ovo polje će biti automatski popunjeno sa ID-om fabrike\n      },\n      errorMessage: '',\n      successMessage: ''\n    };\n  },\n  mounted() {\n    // Kada se komponenta montira, učitavamo ID fabrike iz query parametara\n    const factoryId = this.$route.query.factoryId;\n    if (factoryId) {\n      this.newUser.factoryId = factoryId;\n    } else {\n      this.errorMessage = 'Factory ID is missing!';\n    }\n  },\n  methods: {\n    async createUser() {\n      try {\n        const response = await axios.post('http://localhost:8080/WebShopAppREST/rest/users/createmanager', this.newUser);\n        this.successMessage = 'Manager created successfully!';\n        this.errorMessage = '';\n        this.clearForm();\n      } catch (error) {\n        this.successMessage = '';\n        if (error.response && error.response.data && error.response.data.message) {\n          this.errorMessage = error.response.data.message;\n        } else {\n          this.errorMessage = 'Failed to create manager.';\n        }\n      }\n    },\n    clearForm() {\n      this.newUser = {\n        firstName: '',\n        lastName: '',\n        username: '',\n        password: '',\n        gender: 'MALE',\n        birthDate: '',\n        role: 'MANAGER',\n        customerType: 'NO_TYPE',\n        earnedPoints: 0,\n        factoryId: '' // Resetujemo ID fabrike\n      };\n    }\n  }\n};","map":{"version":3,"names":["axios","data","newUser","firstName","lastName","username","password","gender","birthDate","role","customerType","earnedPoints","factoryId","errorMessage","successMessage","mounted","$route","query","methods","createUser","response","post","clearForm","error","message"],"sources":["C:\\Users\\PC\\eclipse-workspace\\apache-tomcat-9.0.89\\apache-tomcat-9.0.89\\webapps\\chocolate-factory-2\\Kostur\\Frontend\\front-app\\src\\views\\AddManager.vue"],"sourcesContent":["<template>\r\n  <div class=\"d-flex justify-content-center align-items-center vh-75\">\r\n    <div class=\"card p-5 shadow-lg\" style=\"width: 100%; max-width: 500px; margin-top: -50px;\">\r\n     \r\n      <form @submit.prevent=\"createUser\" class=\"manager-form\">\r\n        <div class=\"row mb-3\">\r\n          <label for=\"firstName\" class=\"col-sm-3 col-form-label text-start\">First Name:</label>\r\n          <div class=\"col-sm-9\">\r\n            <input type=\"text\" id=\"firstName\" v-model=\"newUser.firstName\" class=\"form-control\" required>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row mb-3\">\r\n          <label for=\"lastName\" class=\"col-sm-3 col-form-label text-start\">Last Name:</label>\r\n          <div class=\"col-sm-9\">\r\n            <input type=\"text\" id=\"lastName\" v-model=\"newUser.lastName\" class=\"form-control\" required>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row mb-3\">\r\n          <label for=\"username\" class=\"col-sm-3 col-form-label text-start\">Username:</label>\r\n          <div class=\"col-sm-9\">\r\n            <input type=\"text\" id=\"username\" v-model=\"newUser.username\" class=\"form-control\" required>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row mb-3\">\r\n          <label for=\"password\" class=\"col-sm-3 col-form-label text-start\">Password:</label>\r\n          <div class=\"col-sm-9\">\r\n            <input type=\"password\" id=\"password\" v-model=\"newUser.password\" class=\"form-control\" required>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row mb-3\">\r\n          <label for=\"gender\" class=\"col-sm-3 col-form-label text-start\">Gender:</label>\r\n          <div class=\"col-sm-9\">\r\n            <select v-model=\"newUser.gender\" class=\"form-control\" required>\r\n              <option value=\"MALE\">MALE</option>\r\n              <option value=\"FEMALE\">FEMALE</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row mb-3\">\r\n          <label for=\"birthDate\" class=\"col-sm-3 col-form-label text-start\">Birth Date:</label>\r\n          <div class=\"col-sm-9\">\r\n            <input type=\"date\" id=\"birthDate\" v-model=\"newUser.birthDate\" class=\"form-control\" required>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row mb-3\">\r\n          <label for=\"role\" class=\"col-sm-3 col-form-label text-start\">Role:</label>\r\n          <div class=\"col-sm-9\">\r\n            <select v-model=\"newUser.role\" class=\"form-control\" required>\r\n              <option value=\"MANAGER\">MANAGER</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Hidden input za ID fabrike -->\r\n        <input type=\"hidden\" v-model=\"newUser.factoryId\" />\r\n\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <button type=\"submit\" class=\"btn btn-primary w-100\">ADD MANAGER</button>\r\n          </div>\r\n        </div>\r\n      </form>\r\n\r\n      <p v-if=\"errorMessage\" class=\"text-danger text-center mt-3\">{{ errorMessage }}</p>\r\n      <p v-if=\"successMessage\" class=\"text-success text-center mt-3\">{{ successMessage }}</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      newUser: {\r\n        firstName: '',\r\n        lastName: '',\r\n        username: '',\r\n        password: '',\r\n        gender: 'MALE',\r\n        birthDate: '',\r\n        role: 'MANAGER',\r\n        customerType: 'NO_TYPE',\r\n        earnedPoints: 0,\r\n        factoryId: '' // Ovo polje će biti automatski popunjeno sa ID-om fabrike\r\n      },\r\n      errorMessage: '',\r\n      successMessage: ''\r\n    };\r\n  },\r\n  mounted() {\r\n    // Kada se komponenta montira, učitavamo ID fabrike iz query parametara\r\n    const factoryId = this.$route.query.factoryId;\r\n    if (factoryId) {\r\n      this.newUser.factoryId = factoryId;\r\n    } else {\r\n      this.errorMessage = 'Factory ID is missing!';\r\n    }\r\n  },\r\n  methods: {\r\n    async createUser() {\r\n      try {\r\n        const response = await axios.post('http://localhost:8080/WebShopAppREST/rest/users/createmanager', this.newUser);\r\n        this.successMessage = 'Manager created successfully!';\r\n        this.errorMessage = '';\r\n        this.clearForm();\r\n      } catch (error) {\r\n        this.successMessage = '';\r\n        if (error.response && error.response.data && error.response.data.message) {\r\n          this.errorMessage = error.response.data.message;\r\n        } else {\r\n          this.errorMessage = 'Failed to create manager.';\r\n        }\r\n      }\r\n    },\r\n    clearForm() {\r\n      this.newUser = {\r\n        firstName: '',\r\n        lastName: '',\r\n        username: '',\r\n        password: '',\r\n        gender: 'MALE',\r\n        birthDate: '',\r\n        role: 'MANAGER',\r\n        customerType: 'NO_TYPE',\r\n        earnedPoints: 0,\r\n        factoryId: '' // Resetujemo ID fabrike\r\n      };\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.d-flex {\r\n  min-height: 75vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.card {\r\n  border: 1px solid #ddd;\r\n  border-radius: 8px;\r\n  padding: 30px;\r\n  background-color: #f0f8ff;\r\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\nh2 {\r\n  color: #343a40;\r\n  text-align: center;\r\n  font-size: 2rem;\r\n  font-weight: normal;\r\n}\r\n\r\n.form-label {\r\n  margin-bottom: 5px;\r\n  font-weight: normal;\r\n}\r\n\r\ninput[type=\"text\"],\r\ninput[type=\"password\"],\r\ninput[type=\"date\"],\r\nselect,\r\nbutton[type=\"submit\"] {\r\n  border: 1px solid #007bff;\r\n  border-radius: 8px;\r\n  font-size: 1.1rem;\r\n  transition: border-color 0.3s ease, box-shadow 0.3s ease;\r\n}\r\n\r\ninput[type=\"text\"]:focus,\r\ninput[type=\"password\"]:focus,\r\ninput[type=\"date\"]:focus,\r\nselect:focus {\r\n  border-color: #007bff;\r\n  box-shadow: 0 0 5px rgba(0, 123, 255, 0.25);\r\n}\r\n\r\nbutton[type=\"submit\"] {\r\n  border: 1px solid #007bff;\r\n  background-color: transparent;\r\n  color: #007bff;\r\n  transition: background-color 0.3s ease, color 0.3s ease;\r\n}\r\n\r\nbutton[type=\"submit\"]:hover {\r\n  background-color: #007bff;\r\n  color: white;\r\n}\r\n\r\nbutton {\r\n  font-size: 18px;\r\n  margin-top: 20px;\r\n}\r\n\r\n.text-danger {\r\n  margin-top: 15px;\r\n}\r\n\r\n.text-success {\r\n  margin-top: 15px;\r\n}\r\n</style>\r\n"],"mappings":"AA4EA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE,MAAM;QACdC,SAAS,EAAE,EAAE;QACbC,IAAI,EAAE,SAAS;QACfC,YAAY,EAAE,SAAS;QACvBC,YAAY,EAAE,CAAC;QACfC,SAAS,EAAE,EAAC,CAAE;MAChB,CAAC;MACDC,YAAY,EAAE,EAAE;MAChBC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA,MAAMH,SAAQ,GAAI,IAAI,CAACI,MAAM,CAACC,KAAK,CAACL,SAAS;IAC7C,IAAIA,SAAS,EAAE;MACb,IAAI,CAACV,OAAO,CAACU,SAAQ,GAAIA,SAAS;IACpC,OAAO;MACL,IAAI,CAACC,YAAW,GAAI,wBAAwB;IAC9C;EACF,CAAC;EACDK,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMpB,KAAK,CAACqB,IAAI,CAAC,+DAA+D,EAAE,IAAI,CAACnB,OAAO,CAAC;QAChH,IAAI,CAACY,cAAa,GAAI,+BAA+B;QACrD,IAAI,CAACD,YAAW,GAAI,EAAE;QACtB,IAAI,CAACS,SAAS,CAAC,CAAC;MAClB,EAAE,OAAOC,KAAK,EAAE;QACd,IAAI,CAACT,cAAa,GAAI,EAAE;QACxB,IAAIS,KAAK,CAACH,QAAO,IAAKG,KAAK,CAACH,QAAQ,CAACnB,IAAG,IAAKsB,KAAK,CAACH,QAAQ,CAACnB,IAAI,CAACuB,OAAO,EAAE;UACxE,IAAI,CAACX,YAAW,GAAIU,KAAK,CAACH,QAAQ,CAACnB,IAAI,CAACuB,OAAO;QACjD,OAAO;UACL,IAAI,CAACX,YAAW,GAAI,2BAA2B;QACjD;MACF;IACF,CA