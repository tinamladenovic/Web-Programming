{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      chocolate: {\n        name: '',\n        price: 0,\n        type: '',\n        kind: '',\n        weight: 0,\n        description: '',\n        factoryId: '',\n        // factory ID\n        picture: ''\n      },\n      chocolateTypes: ['PLAIN', 'FOR_COOKING', 'FOR_DRINKING'],\n      chocolateKinds: ['DARK', 'MILK', 'WHITE'],\n      isManager: false\n    };\n  },\n  async created() {\n    try {\n      const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/currentUser');\n      const currentUser = response.data;\n      if (currentUser.role === 'MANAGER') {\n        this.isManager = true;\n        this.chocolate.factoryId = this.$route.params.factoryId; // Postavi ID fabrike iz parametara rute\n      } else {\n        this.$router.push('/unauthorized'); // Preusmeravanje ako korisnik nije menadžer\n      }\n    } catch (error) {\n      console.error('Greška pri proveri korisnika:', error);\n      this.$router.push('/login'); // Preusmeravanje na login ako postoji greška\n    }\n  },\n  methods: {\n    async addChocolate() {\n      try {\n        // Postavljanje početnog stanja\n        this.chocolate.quantity = 0;\n        this.chocolate.status = 'OUT_OF_STOCK';\n\n        // Slanje podataka o čokoladi na server\n        await axios.post('http://localhost:8080/WebShopAppREST/rest/chocolates', this.chocolate);\n\n        // Preusmeravanje na stranicu sa svim čokoladama\n        this.$router.push({\n          name: 'allchocolates'\n        });\n      } catch (error) {\n        console.error('Greška pri dodavanju čokolade:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","chocolate","name","price","type","kind","weight","description","factoryId","picture","chocolateTypes","chocolateKinds","isManager","created","response","get","currentUser","role","$route","params","$router","push","error","console","methods","addChocolate","quantity","status","post"],"sources":["C:\\Users\\PC\\eclipse-workspace\\apache-tomcat-9.0.89\\apache-tomcat-9.0.89\\webapps\\chocolate-factory-2\\Kostur\\Frontend\\front-app\\src\\views\\AddChocolate.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"isManager\">\r\n    <h2>Add Chocolate</h2>\r\n    <!-- Forma za dodavanje nove čokolade -->\r\n    <form @submit.prevent=\"addChocolate\" class=\"chocolate-form\">\r\n      <div class=\"form-group\">\r\n        <label for=\"name\">Naziv:</label>\r\n        <input v-model=\"chocolate.name\" id=\"name\" class=\"form-control\" required />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"price\">Cena:</label>\r\n        <input v-model.number=\"chocolate.price\" id=\"price\" type=\"number\" class=\"form-control\" required />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"type\">Tip:</label>\r\n        <select v-model=\"chocolate.type\" id=\"type\" class=\"form-control\" required>\r\n          <option v-for=\"type in chocolateTypes\" :value=\"type\">{{ type }}</option>\r\n        </select>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"kind\">Vrsta:</label>\r\n        <select v-model=\"chocolate.kind\" id=\"kind\" class=\"form-control\" required>\r\n          <option v-for=\"kind in chocolateKinds\" :value=\"kind\">{{ kind }}</option>\r\n        </select>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"weight\">Težina (grama):</label>\r\n        <input v-model.number=\"chocolate.weight\" id=\"weight\" type=\"number\" class=\"form-control\" required />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"description\">Opis:</label>\r\n        <textarea v-model=\"chocolate.description\" id=\"description\" class=\"form-control\" required></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"picture\">Slika:</label>\r\n        <input v-model=\"chocolate.picture\" id=\"picture\" class=\"form-control\" required />\r\n      </div>\r\n      <button type=\"submit\" class=\"btn btn-primary\">Dodaj čokoladu</button>\r\n    </form>\r\n  </div>\r\n  <div v-else>\r\n    <p style=\"color: red;\">You do not have permission to access this page.</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      chocolate: {\r\n        name: '',\r\n        price: 0,\r\n        type: '',\r\n        kind: '',\r\n        weight: 0,\r\n        description: '',\r\n        factoryId: '', // factory ID\r\n        picture: ''\r\n      },\r\n      chocolateTypes: ['PLAIN', 'FOR_COOKING', 'FOR_DRINKING'],\r\n      chocolateKinds: ['DARK', 'MILK', 'WHITE'],\r\n      isManager: false\r\n    };\r\n  },\r\n  async created() {\r\n    try {\r\n      const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/currentUser');\r\n      const currentUser = response.data;\r\n      if (currentUser.role === 'MANAGER') {\r\n        this.isManager = true;\r\n        this.chocolate.factoryId = this.$route.params.factoryId; // Postavi ID fabrike iz parametara rute\r\n      } else {\r\n        this.$router.push('/unauthorized'); // Preusmeravanje ako korisnik nije menadžer\r\n      }\r\n    } catch (error) {\r\n      console.error('Greška pri proveri korisnika:', error);\r\n      this.$router.push('/login'); // Preusmeravanje na login ako postoji greška\r\n    }\r\n  },\r\n  methods: {\r\n    async addChocolate() {\r\n      try {\r\n        // Postavljanje početnog stanja\r\n        this.chocolate.quantity = 0;\r\n        this.chocolate.status = 'OUT_OF_STOCK';\r\n\r\n        // Slanje podataka o čokoladi na server\r\n        await axios.post('http://localhost:8080/WebShopAppREST/rest/chocolates', this.chocolate);\r\n        \r\n        // Preusmeravanje na stranicu sa svim čokoladama\r\n        this.$router.push({ name: 'allchocolates' });\r\n      } catch (error) {\r\n        console.error('Greška pri dodavanju čokolade:', error);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Stilovi za komponentu */\r\n.chocolate-form {\r\n  max-width: 400px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 15px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  font-weight: bold;\r\n}\r\n\r\ninput[type=\"text\"],\r\ninput[type=\"number\"],\r\nselect,\r\ntextarea {\r\n  width: 100%;\r\n  padding: 8px;\r\n  box-sizing: border-box;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n}\r\n\r\ntextarea {\r\n  height: 100px;\r\n}\r\n\r\nbutton {\r\n  width: 100%;\r\n  padding: 10px;\r\n  background-color: #007bff;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n</style>\r\n"],"mappings":";AA8CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;QACTC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,CAAC;QACRC,IAAI,EAAE,EAAE;QACRC,IAAI,EAAE,EAAE;QACRC,MAAM,EAAE,CAAC;QACTC,WAAW,EAAE,EAAE;QACfC,SAAS,EAAE,EAAE;QAAE;QACfC,OAAO,EAAE;MACX,CAAC;MACDC,cAAc,EAAE,CAAC,OAAO,EAAE,aAAa,EAAE,cAAc,CAAC;MACxDC,cAAc,EAAE,CAAC,MAAM,EAAE,MAAM,EAAE,OAAO,CAAC;MACzCC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMC,QAAO,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAAC,uDAAuD,CAAC;MACzF,MAAMC,WAAU,GAAIF,QAAQ,CAACd,IAAI;MACjC,IAAIgB,WAAW,CAACC,IAAG,KAAM,SAAS,EAAE;QAClC,IAAI,CAACL,SAAQ,GAAI,IAAI;QACrB,IAAI,CAACX,SAAS,CAACO,SAAQ,GAAI,IAAI,CAACU,MAAM,CAACC,MAAM,CAACX,SAAS,EAAE;MAC3D,OAAO;QACL,IAAI,CAACY,OAAO,CAACC,IAAI,CAAC,eAAe,CAAC,EAAE;MACtC;IACF,EAAE,OAAOC,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAAC,+BAA+B,EAAEA,KAAK,CAAC;MACrD,IAAI,CAACF,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC,EAAE;IAC/B;EACF,CAAC;EACDG,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF;QACA,IAAI,CAACxB,SAAS,CAACyB,QAAO,GAAI,CAAC;QAC3B