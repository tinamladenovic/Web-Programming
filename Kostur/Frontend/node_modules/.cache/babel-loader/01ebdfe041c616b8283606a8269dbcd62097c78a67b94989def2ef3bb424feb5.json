{"ast":null,"code":"import axios from 'axios';\nimport { ref, onMounted, computed } from 'vue';\nexport default {\n  setup() {\n    const users = ref([]);\n    const sortBy = ref('firstName');\n    const roleFilter = ref('');\n    const searchFirstName = ref('');\n    const searchLastName = ref('');\n    const searchUsername = ref('');\n    const fetchUsers = async () => {\n      try {\n        const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/users', {\n          withCredentials: true\n        });\n        users.value = response.data || []; // Ensure users.value is initialized as an array\n        console.log('Fetched users:', users.value); // Log fetched users\n      } catch (error) {\n        console.error('Error fetching users:', error);\n      }\n    };\n    onMounted(() => {\n      fetchUsers();\n    });\n    const filteredUsers = computed(() => {\n      let filtered = Array.isArray(users.value) ? [...users.value] : [];\n\n      // Filter by role\n      if (roleFilter.value) {\n        filtered = filtered.filter(user => user.role === roleFilter.value);\n      }\n\n      // Search by first name\n      if (searchFirstName.value) {\n        filtered = filtered.filter(user => user.firstName.toLowerCase().includes(searchFirstName.value.toLowerCase()));\n      }\n\n      // Search by last name\n      if (searchLastName.value) {\n        filtered = filtered.filter(user => user.lastName.toLowerCase().includes(searchLastName.value.toLowerCase()));\n      }\n\n      // Search by username\n      if (searchUsername.value) {\n        filtered = filtered.filter(user => user.username.toLowerCase().includes(searchUsername.value.toLowerCase()));\n      }\n\n      // Sort by selected parameter\n      if (sortBy.value) {\n        filtered.sort((a, b) => {\n          if (sortBy.value.startsWith('-')) {\n            const key = sortBy.value.substring(1);\n            return b[key].localeCompare(a[key]);\n          } else {\n            return a[sortBy.value].localeCompare(b[sortBy.value]);\n          }\n        });\n      }\n      return filtered;\n    });\n    const clearSearch = () => {\n      searchFirstName.value = '';\n      searchLastName.value = '';\n      searchUsername.value = '';\n    };\n    return {\n      filteredUsers,\n      sortBy,\n      roleFilter,\n      searchFirstName,\n      searchLastName,\n      searchUsername,\n      clearSearch\n    };\n  }\n};","map":{"version":3,"names":["axios","ref","onMounted","computed","setup","users","sortBy","roleFilter","searchFirstName","searchLastName","searchUsername","fetchUsers","response","get","withCredentials","value","data","console","log","error","filteredUsers","filtered","Array","isArray","filter","user","role","firstName","toLowerCase","includes","lastName","username","sort","a","b","startsWith","key","substring","localeCompare","clearSearch"],"sources":["C:\\Users\\PC\\eclipse-workspace\\apache-tomcat-9.0.89\\apache-tomcat-9.0.89\\webapps\\chocolate-factory-2\\Kostur\\Frontend\\front-app\\src\\views\\UserTable.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Registered Users</h2>\r\n\r\n    <!-- Search input fields -->\r\n    <div>\r\n      <label>Search by:</label>\r\n      <input type=\"text\" v-model=\"searchFirstName\" placeholder=\"First Name\">\r\n      <input type=\"text\" v-model=\"searchLastName\" placeholder=\"Last Name\">\r\n      <input type=\"text\" v-model=\"searchUsername\" placeholder=\"Username\">\r\n      <button @click=\"clearSearch\">Clear</button>\r\n    </div>\r\n\r\n    <!-- Sorting controls -->\r\n    <div>\r\n      <label>Sort by:</label>\r\n      <select v-model=\"sortBy\">\r\n        <option value=\"firstName\">First Name (A-Z)</option>\r\n        <option value=\"-firstName\">First Name (Z-A)</option>\r\n        <option value=\"lastName\">Last Name (A-Z)</option>\r\n        <option value=\"-lastName\">Last Name (Z-A)</option>\r\n        <option value=\"username\">Username (A-Z)</option>\r\n        <option value=\"-username\">Username (Z-A)</option>\r\n        <option value=\"earnedPoints\">Earned Points (Ascending)</option>\r\n        <option value=\"-earnedPoints\">Earned Points (Descending)</option>\r\n      </select>\r\n    </div>\r\n\r\n    <!-- Filtering controls -->\r\n    <div>\r\n      <label>Filter by role:</label>\r\n      <select v-model=\"roleFilter\">\r\n        <option value=\"\">All</option>\r\n        <option value=\"ADMIN\">Admin</option>\r\n        <option value=\"CUSTOMER\">Customer</option>\r\n        <option value=\"WORKER\">Worker</option>\r\n        <option value=\"MANAGER\">Manager</option>\r\n      </select>\r\n    </div>\r\n\r\n    <table class=\"table\">\r\n      <thead>\r\n        <tr>\r\n          <th>First Name</th>\r\n          <th>Last Name</th>\r\n          <th>Username</th>\r\n          <th>Earned Points</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"user in filteredUsers\" :key=\"user.id\">\r\n          <td>{{ user.firstName }}</td>\r\n          <td>{{ user.lastName }}</td>\r\n          <td>{{ user.username }}</td>\r\n          <td>{{ user.earnedPoints }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { ref, onMounted, computed } from 'vue';\r\n\r\nexport default {\r\n  setup() {\r\n    const users = ref([]);\r\n    const sortBy = ref('firstName');\r\n    const roleFilter = ref('');\r\n    const searchFirstName = ref('');\r\n    const searchLastName = ref('');\r\n    const searchUsername = ref('');\r\n\r\n    const fetchUsers = async () => {\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/users', {\r\n          withCredentials: true,\r\n        });\r\n        users.value = response.data || []; // Ensure users.value is initialized as an array\r\n        console.log('Fetched users:', users.value); // Log fetched users\r\n      } catch (error) {\r\n        console.error('Error fetching users:', error);\r\n      }\r\n    };\r\n\r\n    onMounted(() => {\r\n      fetchUsers();\r\n    });\r\n\r\n    const filteredUsers = computed(() => {\r\n      let filtered = Array.isArray(users.value) ? [...users.value] : [];\r\n\r\n      // Filter by role\r\n      if (roleFilter.value) {\r\n        filtered = filtered.filter(user => user.role === roleFilter.value);\r\n      }\r\n\r\n      // Search by first name\r\n      if (searchFirstName.value) {\r\n        filtered = filtered.filter(user => user.firstName.toLowerCase().includes(searchFirstName.value.toLowerCase()));\r\n      }\r\n\r\n      // Search by last name\r\n      if (searchLastName.value) {\r\n        filtered = filtered.filter(user => user.lastName.toLowerCase().includes(searchLastName.value.toLowerCase()));\r\n      }\r\n\r\n      // Search by username\r\n      if (searchUsername.value) {\r\n        filtered = filtered.filter(user => user.username.toLowerCase().includes(searchUsername.value.toLowerCase()));\r\n      }\r\n\r\n      // Sort by selected parameter\r\n      if (sortBy.value) {\r\n        filtered.sort((a, b) => {\r\n          if (sortBy.value.startsWith('-')) {\r\n            const key = sortBy.value.substring(1);\r\n            return b[key].localeCompare(a[key]);\r\n          } else {\r\n            return a[sortBy.value].localeCompare(b[sortBy.value]);\r\n          }\r\n        });\r\n      }\r\n\r\n      return filtered;\r\n    });\r\n\r\n    const clearSearch = () => {\r\n      searchFirstName.value = '';\r\n      searchLastName.value = '';\r\n      searchUsername.value = '';\r\n    };\r\n\r\n    return {\r\n      filteredUsers,\r\n      sortBy,\r\n      roleFilter,\r\n      searchFirstName,\r\n      searchLastName,\r\n      searchUsername,\r\n      clearSearch,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\n\r\n.table th, .table td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n}\r\n\r\n.table th {\r\n  background-color: #f2f2f2;\r\n  text-align: left;\r\n}\r\n</style>\r\n"],"mappings":"AA8DA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,GAAG,EAAEC,SAAS,EAAEC,QAAO,QAAS,KAAK;AAE9C,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIJ,GAAG,CAAC,EAAE,CAAC;IACrB,MAAMK,MAAK,GAAIL,GAAG,CAAC,WAAW,CAAC;IAC/B,MAAMM,UAAS,GAAIN,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMO,eAAc,GAAIP,GAAG,CAAC,EAAE,CAAC;IAC/B,MAAMQ,cAAa,GAAIR,GAAG,CAAC,EAAE,CAAC;IAC9B,MAAMS,cAAa,GAAIT,GAAG,CAAC,EAAE,CAAC;IAE9B,MAAMU,UAAS,GAAI,MAAAA,CAAA,KAAY;MAC7B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAAC,iDAAiD,EAAE;UAClFC,eAAe,EAAE;QACnB,CAAC,CAAC;QACFT,KAAK,CAACU,KAAI,GAAIH,QAAQ,CAACI,IAAG,IAAK,EAAE,EAAE;QACnCC,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEb,KAAK,CAACU,KAAK,CAAC,EAAE;MAC9C,EAAE,OAAOI,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C;IACF,CAAC;IAEDjB,SAAS,CAAC,MAAM;MACdS,UAAU,CAAC,CAAC;IACd,CAAC,CAAC;IAEF,MAAMS,aAAY,GAAIjB,QAAQ,CAAC,MAAM;MACnC,IAAIkB,QAAO,GAAIC,KAAK,CAACC,OAAO,CAAClB,KAAK,CAACU,KAAK,IAAI,CAAC,GAAGV,KAAK,CAACU,KAAK,IAAI,EAAE;;MAEjE;MACA,IAAIR,UAAU,CAACQ,KAAK,EAAE;QACpBM,QAAO,GAAIA,QAAQ,CAACG,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,IAAG,KAAMnB,UAAU,CAACQ,KAAK,CAAC;MACpE;;MAEA;MACA,IAAIP,eAAe,CAACO,KAAK,EAAE;QACzBM,QAAO,GAAIA,QAAQ,CAACG,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACE,SAAS,CAACC,WAAW,CAAC,CAAC,CAACC,QAAQ,CAACrB,eAAe,CAACO,KAAK,CAACa,WAAW,CAAC,CAAC,CAAC,CAAC;MAChH;;MAEA;MACA,IAAInB,cAAc,CAACM,KAAK,EAAE;QACxBM,QAAO,GAAIA,QAAQ,CAACG,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACK,QAAQ,CAACF,WAAW,CAAC,CAAC,CAACC,QAAQ,CAACpB,cAAc,CAACM,KAAK,CAACa,WAAW,CAAC,CAAC,CAAC,CAAC;MAC9G;;MAEA;MACA,IAAIlB,cAAc,CAACK,KAAK,EAAE;QACxBM,QAAO,GAAIA,QAAQ,CAACG,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACM,QAAQ,CAACH,WAAW,CAAC,CAAC,CAACC,QAAQ,CAACnB,cAAc,CAACK,KAAK,CAACa,WAAW,CAAC,CAAC,CAAC,CAAC;MAC9G;;MAEA;MACA,IAAItB,MAAM,CAACS,KAAK,EAAE;QAChBM,QAAQ,CAACW,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;UACtB,IAAI5B,MAAM,CAACS,KAAK,CAACoB,UAAU,CAAC,GAAG,CAAC,EAAE;YAChC,MAAMC,GAAE,GAAI9B,MAAM,CAACS,KAAK,CAACsB,SAAS,CAAC,CAAC,CAAC;YACrC,