{"ast":null,"code":"import axios from 'axios';\nimport { useRouter, useRoute } from 'vue-router';\nexport default {\n  data() {\n    return {\n      newWorker: {\n        firstName: '',\n        lastName: '',\n        username: '',\n        password: '',\n        gender: 'MALE',\n        birthDate: '',\n        role: 'WORKER',\n        customerType: {\n          name: 'NO_TYPE',\n          discount: 0.0,\n          requiredPoints: 0\n        },\n        earnedPoints: 0,\n        shoppingCartId: null,\n        blocked: false,\n        factoryId: null\n      },\n      errorMessage: '',\n      successMessage: ''\n    };\n  },\n  async created() {\n    const route = useRoute();\n    if (route.params.factoryId) {\n      this.newWorker.factoryId = route.params.factoryId; // Ako imamo factoryId iz rute\n    } else {\n      // Ako nemamo factoryId, zatraži od servera\n      try {\n        const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/users/currentManagerFactory', {\n          withCredentials: true\n        });\n        if (response.data && response.data.factoryId) {\n          this.newWorker.factoryId = response.data.factoryId; // Postavljanje factoryId\n        } else {\n          this.errorMessage = 'Failed to fetch factory for manager.';\n        }\n      } catch (error) {\n        this.errorMessage = 'Failed to fetch factory for manager.';\n      }\n    }\n  },\n  methods: {\n    async createWorker() {\n      try {\n        console.log(\"Worker data being sent:\", this.newWorker);\n        const response = await axios.post('http://localhost:8080/WebShopAppREST/rest/users/createWorker', this.newWorker, {\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          withCredentials: true\n        });\n        this.successMessage = 'Worker created successfully!';\n        this.errorMessage = '';\n        this.clearForm();\n      } catch (error) {\n        if (error.response) {\n          console.error('Error response:', error.response.data);\n          this.errorMessage = `Failed to create worker: ${error.response.data}`;\n        } else {\n          console.error('Error:', error);\n          this.errorMessage = 'Failed to create worker. Please try again.';\n        }\n        this.successMessage = '';\n      }\n    },\n    clearForm() {\n      const route = useRoute();\n      this.newWorker = {\n        firstName: '',\n        lastName: '',\n        username: '',\n        password: '',\n        gender: 'MALE',\n        birthDate: '',\n        role: 'WORKER',\n        customerType: {\n          name: 'NO_TYPE',\n          discount: 0.0,\n          requiredPoints: 0\n        },\n        earnedPoints: 0,\n        shoppingCartId: null,\n        blocked: false,\n        factoryId: route.params.factoryId || this.newWorker.factoryId // Ponovo postavljamo factoryId\n      };\n    }\n  }\n};","map":{"version":3,"names":["axios","useRouter","useRoute","data","newWorker","firstName","lastName","username","password","gender","birthDate","role","customerType","name","discount","requiredPoints","earnedPoints","shoppingCartId","blocked","factoryId","errorMessage","successMessage","created","route","params","response","get","withCredentials","error","methods","createWorker","console","log","post","headers","clearForm"],"sources":["C:\\Users\\PC\\eclipse-workspace\\apache-tomcat-9.0.89\\apache-tomcat-9.0.89\\webapps\\chocolate-factory-2\\Kostur\\Frontend\\front-app\\src\\views\\AddWorker.vue"],"sourcesContent":["<template>\r\n  <div class=\"d-flex justify-content-center align-items-center vh-75\">\r\n    <div class=\"card p-5 shadow-lg\" style=\"width: 100%; max-width: 500px; margin-top: -50px;\">\r\n      \r\n      <form @submit.prevent=\"createWorker\" class=\"worker-form\">\r\n        <!-- Polja za unos -->\r\n        <div class=\"row mb-3\">\r\n          <label for=\"firstName\" class=\"col-sm-3 col-form-label text-start\">First Name:</label>\r\n          <div class=\"col-sm-9\">\r\n            <input type=\"text\" id=\"firstName\" v-model=\"newWorker.firstName\" class=\"form-control\" autocomplete=\"given-name\" required>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row mb-3\">\r\n          <label for=\"lastName\" class=\"col-sm-3 col-form-label text-start\">Last Name:</label>\r\n          <div class=\"col-sm-9\">\r\n            <input type=\"text\" id=\"lastName\" v-model=\"newWorker.lastName\" class=\"form-control\" autocomplete=\"family-name\" required>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row mb-3\">\r\n          <label for=\"username\" class=\"col-sm-3 col-form-label text-start\">Username:</label>\r\n          <div class=\"col-sm-9\">\r\n            <input type=\"text\" id=\"username\" v-model=\"newWorker.username\" class=\"form-control\" autocomplete=\"username\" required>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row mb-3\">\r\n          <label for=\"password\" class=\"col-sm-3 col-form-label text-start\">Password:</label>\r\n          <div class=\"col-sm-9\">\r\n            <input type=\"password\" id=\"password\" v-model=\"newWorker.password\" class=\"form-control\" autocomplete=\"new-password\" required>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row mb-3\">\r\n          <label for=\"gender\" class=\"col-sm-3 col-form-label text-start\">Gender:</label>\r\n          <div class=\"col-sm-9\">\r\n            <select id=\"gender\" v-model=\"newWorker.gender\" class=\"form-control\" required>\r\n              <option value=\"MALE\">MALE</option>\r\n              <option value=\"FEMALE\">FEMALE</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row mb-3\">\r\n          <label for=\"birthDate\" class=\"col-sm-3 col-form-label text-start\">Birth Date:</label>\r\n          <div class=\"col-sm-9\">\r\n            <input type=\"date\" id=\"birthDate\" v-model=\"newWorker.birthDate\" class=\"form-control\" required>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Role - zaključano polje -->\r\n        <div class=\"row mb-3\">\r\n          <label for=\"role\" class=\"col-sm-3 col-form-label text-start\">Role:</label>\r\n          <div class=\"col-sm-9\">\r\n            <select id=\"role\" v-model=\"newWorker.role\" class=\"form-control\" disabled>\r\n              <option value=\"WORKER\">WORKER</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Customer Type - zaključano polje -->\r\n        <div class=\"row mb-3\">\r\n          <label for=\"customerType\" class=\"col-sm-3 col-form-label text-start\">Customer Type:</label>\r\n          <div class=\"col-sm-9\">\r\n            <select id=\"customerType\" v-model=\"newWorker.customerType.name\" class=\"form-control\" disabled>\r\n              <option value=\"NO_TYPE\">NO_TYPE</option>\r\n              <option value=\"BRONZE\">BRONZE</option>\r\n              <option value=\"SILVER\">SILVER</option>\r\n              <option value=\"GOLD\">GOLD</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Earned Points - zaključano polje -->\r\n        <div class=\"row mb-3\">\r\n          <label for=\"earnedPoints\" class=\"col-sm-3 col-form-label text-start\">Earned Points:</label>\r\n          <div class=\"col-sm-9\">\r\n            <input type=\"number\" id=\"earnedPoints\" v-model=\"newWorker.earnedPoints\" class=\"form-control\" disabled>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <button type=\"submit\" class=\"btn btn-primary w-100\">ADD WORKER</button>\r\n          </div>\r\n        </div>\r\n      </form>\r\n\r\n      <p v-if=\"errorMessage\" class=\"text-danger text-center mt-3\">{{ errorMessage }}</p>\r\n      <p v-if=\"successMessage\" class=\"text-success text-center mt-3\">{{ successMessage }}</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { useRouter, useRoute } from 'vue-router';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      newWorker: {\r\n        firstName: '',\r\n        lastName: '',\r\n        username: '',\r\n        password: '',\r\n        gender: 'MALE',\r\n        birthDate: '',\r\n        role: 'WORKER',\r\n        customerType: {\r\n          name: 'NO_TYPE',\r\n          discount: 0.0,\r\n          requiredPoints: 0\r\n        },\r\n        earnedPoints: 0,\r\n        shoppingCartId: null,\r\n        blocked: false,\r\n        factoryId: null \r\n      },\r\n      errorMessage: '',\r\n      successMessage: ''\r\n    };\r\n  },\r\n  async created() {\r\n    const route = useRoute();\r\n    if (route.params.factoryId) {\r\n      this.newWorker.factoryId = route.params.factoryId; // Ako imamo factoryId iz rute\r\n    } else {\r\n      // Ako nemamo factoryId, zatraži od servera\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/users/currentManagerFactory', { withCredentials: true });\r\n        if (response.data && response.data.factoryId) {\r\n          this.newWorker.factoryId = response.data.factoryId; // Postavljanje factoryId\r\n        } else {\r\n          this.errorMessage = 'Failed to fetch factory for manager.';\r\n        }\r\n      } catch (error) {\r\n        this.errorMessage = 'Failed to fetch factory for manager.';\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    async createWorker() {\r\n      try {\r\n        console.log(\"Worker data being sent:\", this.newWorker);\r\n        const response = await axios.post('http://localhost:8080/WebShopAppREST/rest/users/createWorker', this.newWorker, {\r\n          headers: {\r\n            'Content-Type': 'application/json',\r\n          },\r\n          withCredentials: true\r\n        });\r\n        this.successMessage = 'Worker created successfully!';\r\n        this.errorMessage = '';\r\n        this.clearForm();\r\n      } catch (error) {\r\n        if (error.response) {\r\n          console.error('Error response:', error.response.data);\r\n          this.errorMessage = `Failed to create worker: ${error.response.data}`;\r\n        } else {\r\n          console.error('Error:', error);\r\n          this.errorMessage = 'Failed to create worker. Please try again.';\r\n        }\r\n        this.successMessage = '';\r\n      }\r\n    },\r\n    clearForm() {\r\n      const route = useRoute();\r\n      this.newWorker = {\r\n        firstName: '',\r\n        lastName: '',\r\n        username: '',\r\n        password: '',\r\n        gender: 'MALE',\r\n        birthDate: '',\r\n        role: 'WORKER',\r\n        customerType: {\r\n          name: 'NO_TYPE',\r\n          discount: 0.0,\r\n          requiredPoints: 0\r\n        },\r\n        earnedPoints: 0,\r\n        shoppingCartId: null,\r\n        blocked: false,\r\n        factoryId: route.params.factoryId || this.newWorker.factoryId // Ponovo postavljamo factoryId\r\n      };\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.d-flex {\r\n  min-height: 75vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.card {\r\n  border: 1px solid #ddd;\r\n  border-radius: 8px;\r\n  padding: 30px;\r\n  background-color: #f0f8ff;\r\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\nh2 {\r\n  color: #343a40;\r\n  text-align: center;\r\n  font-size: 2rem;\r\n  font-weight: normal;\r\n}\r\n\r\n.form-label {\r\n  font-weight: normal;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n/* Stilizacija za inpute i dugmad */\r\ninput[type=\"text\"],\r\ninput[type=\"password\"],\r\ninput[type=\"date\"],\r\ninput[type=\"number\"],\r\nselect,\r\nbutton[type=\"submit\"] {\r\n  border: 1px solid #007bff;\r\n  border-radius: 8px;\r\n  font-size: 1.1rem;\r\n  transition: border-color 0.3s ease, box-shadow 0.3s ease;\r\n}\r\n\r\n/* Fokus efekat na inputima */\r\ninput[type=\"text\"]:focus,\r\ninput[type=\"password\"]:focus,\r\ninput[type=\"date\"]:focus,\r\ninput[type=\"number\"]:focus,\r\nselect:focus {\r\n  border-color: #007bff;\r\n  box-shadow: 0 0 5px rgba(0, 123, 255, 0.25);\r\n}\r\n\r\n/* Dugmad sa plavim outline stilom */\r\nbutton[type=\"submit\"] {\r\n  border: 1px solid #007bff;\r\n  background-color: transparent;\r\n  color: #007bff;\r\n  transition: background-color 0.3s ease, color 0.3s ease;\r\n}\r\n\r\n/* Hover efekat na dugmad */\r\nbutton[type=\"submit\"]:hover {\r\n  background-color: #007bff;\r\n  color: white;\r\n}\r\n\r\nbutton {\r\n  font-size: 18px;\r\n  margin-top: 20px;\r\n}\r\n\r\n/* Stilizacija za uspešne i greške poruke */\r\n.text-danger {\r\n  margin-top: 15px;\r\n}\r\n\r\n.text-success {\r\n  margin-top: 15px;\r\n}\r\n</style>\r\n"],"mappings":"AAgGA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,SAAS,EAAEC,QAAO,QAAS,YAAY;AAEhD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;QACTC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE,MAAM;QACdC,SAAS,EAAE,EAAE;QACbC,IAAI,EAAE,QAAQ;QACdC,YAAY,EAAE;UACZC,IAAI,EAAE,SAAS;UACfC,QAAQ,EAAE,GAAG;UACbC,cAAc,EAAE;QAClB,CAAC;QACDC,YAAY,EAAE,CAAC;QACfC,cAAc,EAAE,IAAI;QACpBC,OAAO,EAAE,KAAK;QACdC,SAAS,EAAE;MACb,CAAC;MACDC,YAAY,EAAE,EAAE;MAChBC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAMC,KAAI,GAAIrB,QAAQ,CAAC,CAAC;IACxB,IAAIqB,KAAK,CAACC,MAAM,CAACL,SAAS,EAAE;MAC1B,IAAI,CAACf,SAAS,CAACe,SAAQ,GAAII,KAAK,CAACC,MAAM,CAACL,SAAS,EAAE;IACrD,OAAO;MACL;MACA,IAAI;QACF,MAAMM,QAAO,GAAI,MAAMzB,KAAK,CAAC0B,GAAG,CAAC,uEAAuE,EAAE;UAAEC,eAAe,EAAE;QAAK,CAAC,CAAC;QACpI,IAAIF,QAAQ,CAACtB,IAAG,IAAKsB,QAAQ,CAACtB,IAAI,CAACgB,SAAS,EAAE;UAC5C,IAAI,CAACf,SAAS,CAACe,SAAQ,GAAIM,QAAQ,CAACtB,IAAI,CAACgB,SAAS,EAAE;QACtD,OAAO;UACL,IAAI,CAACC,YAAW,GAAI,sCAAsC;QAC5D;MACF,EAAE,OAAOQ,KAAK,EAAE;QACd,IAAI,CAACR,YAAW,GAAI,sCAAsC;MAC5D;IACF;EACF,CAAC;EACDS,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI;QACFC,OAAO,CAACC,GAAG,CAAC,yBAAyB,EAAE,IAAI,CAAC5B,SAAS,CAAC;QACtD,MAAMqB,QAAO,GAAI,MAAMzB,KA