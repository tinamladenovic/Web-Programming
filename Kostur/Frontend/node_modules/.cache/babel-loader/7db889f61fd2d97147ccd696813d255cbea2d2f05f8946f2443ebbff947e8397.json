{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { useRouter } from 'vue-router';\nexport default {\n  data() {\n    return {\n      chocolate: {\n        name: '',\n        price: 0,\n        type: '',\n        kind: '',\n        weight: 0,\n        description: '',\n        factory: '',\n        picture: ''\n      },\n      chocolateTypes: ['PLAIN', 'FOR_COOKING', 'FOR_DRINKING'],\n      chocolateKinds: ['DARK', 'MILK', 'WHITE']\n    };\n  },\n  methods: {\n    async addChocolate() {\n      try {\n        // Postavljanje početnog stanja\n        this.chocolate.quantity = 0;\n        this.chocolate.status = 'OUT_OF_STOCK';\n\n        // Slanje podataka o čokoladi na server\n        await axios.post('http://localhost:8080/WebShopAppREST/rest/chocolates', this.chocolate);\n\n        // Preusmeravanje na stranicu sa svim čokoladama\n        this.$router.push({\n          name: 'allchocolates'\n        });\n      } catch (error) {\n        console.error('Greška pri dodavanju čokolade:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","useRouter","data","chocolate","name","price","type","kind","weight","description","factory","picture","chocolateTypes","chocolateKinds","methods","addChocolate","quantity","status","post","$router","push","error","console"],"sources":["C:\\Users\\PC\\eclipse-workspace\\apache-tomcat-9.0.89\\apache-tomcat-9.0.89\\webapps\\chocolate-factory-2\\Kostur\\Frontend\\front-app\\src\\views\\AddChocolate.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Add Chocolate</h2>\r\n    <!-- Forma za dodavanje nove čokolade -->\r\n    <form @submit.prevent=\"addChocolate\">\r\n      <div>\r\n        <label for=\"name\">Naziv:</label>\r\n        <input v-model=\"chocolate.name\" id=\"name\" required />\r\n      </div>\r\n      <div>\r\n        <label for=\"price\">Cena:</label>\r\n        <input v-model.number=\"chocolate.price\" id=\"price\" type=\"number\" required />\r\n      </div>\r\n      <div>\r\n        <label for=\"type\">Tip:</label>\r\n        <select v-model=\"chocolate.type\" id=\"type\" required>\r\n          <option v-for=\"type in chocolateTypes\" :value=\"type\">{{ type }}</option>\r\n        </select>\r\n      </div>\r\n      <div>\r\n        <label for=\"kind\">Vrsta:</label>\r\n        <select v-model=\"chocolate.kind\" id=\"kind\" required>\r\n          <option v-for=\"kind in chocolateKinds\" :value=\"kind\">{{ kind }}</option>\r\n        </select>\r\n      </div>\r\n      <div>\r\n        <label for=\"weight\">Težina (grama):</label>\r\n        <input v-model.number=\"chocolate.weight\" id=\"weight\" type=\"number\" required />\r\n      </div>\r\n      <div>\r\n        <label for=\"description\">Opis:</label>\r\n        <textarea v-model=\"chocolate.description\" id=\"description\" required></textarea>\r\n      </div>\r\n      <div>\r\n        <label for=\"factory\">Fabrika:</label>\r\n        <input v-model=\"chocolate.factory\" id=\"factory\" required />\r\n      </div>\r\n      <div>\r\n        <label for=\"picture\">Slika:</label>\r\n        <input v-model=\"chocolate.picture\" id=\"picture\" required />\r\n      </div>\r\n      <button type=\"submit\">Dodaj čokoladu</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { useRouter } from 'vue-router';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      chocolate: {\r\n        name: '',\r\n        price: 0,\r\n        type: '',\r\n        kind: '',\r\n        weight: 0,\r\n        description: '',\r\n        factory: '',\r\n        picture: ''\r\n      },\r\n      chocolateTypes: ['PLAIN', 'FOR_COOKING', 'FOR_DRINKING'],\r\n      chocolateKinds: ['DARK', 'MILK', 'WHITE']\r\n    };\r\n  },\r\n  methods: {\r\n    async addChocolate() {\r\n      try {\r\n        // Postavljanje početnog stanja\r\n        this.chocolate.quantity = 0;\r\n        this.chocolate.status = 'OUT_OF_STOCK';\r\n\r\n        // Slanje podataka o čokoladi na server\r\n        await axios.post('http://localhost:8080/WebShopAppREST/rest/chocolates', this.chocolate);\r\n        \r\n        // Preusmeravanje na stranicu sa svim čokoladama\r\n        this.$router.push({ name: 'allchocolates' });\r\n      } catch (error) {\r\n        console.error('Greška pri dodavanju čokolade:', error);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Stilovi za komponentu */\r\n</style>\r\n"],"mappings":";AA+CA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,YAAY;AAEtC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;QACTC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,CAAC;QACRC,IAAI,EAAE,EAAE;QACRC,IAAI,EAAE,EAAE;QACRC,MAAM,EAAE,CAAC;QACTC,WAAW,EAAE,EAAE;QACfC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE;MACX,CAAC;MACDC,cAAc,EAAE,CAAC,OAAO,EAAE,aAAa,EAAE,cAAc,CAAC;MACxDC,cAAc,EAAE,CAAC,MAAM,EAAE,MAAM,EAAE,OAAO;IAC1C,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF;QACA,IAAI,CAACZ,SAAS,CAACa,QAAO,GAAI,CAAC;QAC3B,IAAI,CAACb,SAAS,CAACc,MAAK,GAAI,cAAc;;QAEtC;QACA,MAAMjB,KAAK,CAACkB,IAAI,CAAC,sDAAsD,EAAE,IAAI,CAACf,SAAS,CAAC;;QAExF;QACA,IAAI,CAACgB,OAAO,CAACC,IAAI,CAAC;UAAEhB,IAAI,EAAE;QAAgB,CAAC,CAAC;MAC9C,EAAE,OAAOiB,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,gCAAgC,EAAEA,KAAK,CAAC;MACxD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}