chocolates');\r\n        console.log('Loaded chocolates:', response.data);\r\n        this.chocolates = response.data;\r\n      } catch (error) {\r\n        console.error('Greška pri preuzimanju čokolada:', error);\r\n      }\r\n    },\r\n    async fetchFactories() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/chocolateFactoryObjects');\r\n        console.log('Loaded factories:', response.data);\r\n        this.factories = response.data;\r\n      } catch (error) {\r\n        console.error('Greška pri preuzimanju fabrika:', error);\r\n      }\r\n    },\r\n    getFactoryName(factoryId) {\r\n      const factory = this.factories.find(f => f.factoryId === factoryId);\r\n      console.log('Factory ID:', factoryId, 'Factory:', factory);\r\n      return factory ? factory.name : 'Nepoznato';\r\n    },\r\n    getChocolatePicture(pictureName) {\r\n      return `/images/${pictureName}`;\r\n    },\r\n    editChocolate(id) {\r\n      this.$router.push({ name: 'editchocolate', params: { id } });\r\n    },\r\n    async deleteChocolate(id) {\r\n      try {\r\n        await axios.delete(`http://localhost:8080/WebShopAppREST/rest/chocolates/${id}`);\r\n        this.chocolates = this.chocolates.filter(chocolate => chocolate.id !== id);\r\n      } catch (error) {\r\n        console.error('Greška pri brisanju čokolade:', error);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.all-chocolates {\r\n  overflow-x: auto;\r\n  padding: 20px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-bottom: 20px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\nth, td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n  text-align: left;\r\n}\r\n\r\nth {\r\n  background-color: #f2f2f2;\r\n  cursor: pointer;\r\n}\r\n\r\n.chocolate-picture {\r\n  width: 50px;\r\n  height: 50px;\r\n  object-fit: cover;\r\n}\r\n\r\nbutton {\r\n  padding: 5px 10px;\r\n  margin: 2px;\r\n  background-color: #f5e7f5;\r\n  color: #333;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #d8bfd8;\r\n  color: #fff;\r\n}\r\n</style>\r\n"],"mappings":";AAoCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,cAAc,CAAC,CAAC;IAC3B,MAAM,IAAI,CAACC,eAAe,CAAC,CAAC;EAC9B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMR,KAAK,CAACS,GAAG,CAAC,sDAAsD,CAAC;QACxFC,OAAO,CAACC,GAAG,CAAC,oBAAoB,EAAEH,QAAQ,CAACP,IAAI,CAAC;QAChD,IAAI,CAACC,UAAS,GAAIM,QAAQ,CAACP,IAAI;MACjC,EAAE,OAAOW,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAAC,kCAAkC,EAAEA,KAAK,CAAC;MAC1D;IACF,CAAC;IACD,MAAMP,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMR,KAAK,CAACS,GAAG,CAAC,mEAAmE,CAAC;QACrGC,OAAO,CAACC,GAAG,CAAC,mBAAmB,EAAEH,QAAQ,CAACP,IAAI,CAAC;QAC/C,IAAI,CAACE,SAAQ,GAAIK,QAAQ,CAACP,IAAI;MAChC,EAAE,OAAOW,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;MACzD;IACF,CAAC;IACDC,cAAcA,CAACC,SAAS,EAAE;MACxB,MAAMC,OAAM,GAAI,IAAI,CAACZ,SAAS,CAACa,IAAI,CAACC,CAAA,IAAKA,CAAC,CAACH,SAAQ,KAAMA,SAAS,CAAC;MACnEJ,OAAO,CAACC,GAAG,CAAC,aAAa,EAAEG,SAAS,EAAE,UAAU,EAAEC,OAAO,CAAC;MAC1D,OAAOA,OAAM,GAAIA,OAAO,CAACG,IAAG,GAAI,WAAW;IAC7C,CAAC;IACDC,mBAAmBA,CAACC,WAAW,EAAE;MAC/B,OAAQ,WAAUA,WAAY,EAAC;IACjC,CAAC;IACDC,aAAaA,CAACC,EAAE,EAAE;MAChB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEN,IAAI,EAAE,eAAe;QAAEO,MAAM,EAAE;UAAEH;QAAG;MAAE,CAAC,CAAC;IAC9D,CAAC;IACD,MAAMI,eAAeA,CAACJ,EAAE,EAAE;MACxB,IAAI;QACF,MAAMtB,KAAK,CAAC2B,MAAM,CAAE,wDAAuDL,EAAG,EAAC,CAAC;QAChF,IAAI,CAACpB,UAAS,GAAI,IAAI,CAACA,UAAU,CAAC0B,MAAM,CAACC,SAAQ,IAAKA,SAAS,CAACP,EAAC,KAAMA,EAAE,CAAC;MAC5E,EAAE,OAAOV,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAAC,+BAA+B,EAAEA,KAAK,CAAC;MACvD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"mo