{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      chocolates: [],\n      factories: []\n    };\n  },\n  async created() {\n    await this.fetchFactories();\n    await this.fetchChocolates();\n  },\n  methods: {\n    async fetchChocolates() {\n      try {\n        const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/chocolates');\n        console.log('Loaded chocolates:', response.data);\n        this.chocolates = response.data;\n      } catch (error) {\n        console.error('Greška pri preuzimanju čokolada:', error);\n      }\n    },\n    async fetchFactories() {\n      try {\n        const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/chocolateFactoryObjects');\n        console.log('Loaded factories:', response.data);\n        this.factories = response.data;\n      } catch (error) {\n        console.error('Greška pri preuzimanju fabrika:', error);\n      }\n    },\n    getFactoryName(factoryId) {\n      const factory = this.factories.find(f => f.factoryId === factoryId);\n      console.log('Factory ID:', factoryId, 'Factory:', factory);\n      return factory ? factory.name : 'Nepoznato';\n    },\n    getChocolatePicture(pictureName) {\n      return `/images/${pictureName}`;\n    },\n    editChocolate(id) {\n      this.$router.push({\n        name: 'editchocolate',\n        params: {\n          id\n        }\n      });\n    },\n    async deleteChocolate(id) {\n      try {\n        await axios.delete(`http://localhost:8080/WebShopAppREST/rest/chocolates/${id}`);\n        this.chocolates = this.chocolates.filter(chocolate => chocolate.id !== id);\n      } catch (error) {\n        console.error('Greška pri brisanju čokolade:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","chocolates","factories","created","fetchFactories","fetchChocolates","methods","response","get","console","log","error","getFact