{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { useRouter } from 'vue-router';\nexport default {\n  data() {\n    return {\n      chocolates: []\n    };\n  },\n  async created() {\n    await this.fetchChocolates();\n  },\n  methods: {\n    async fetchChocolates() {\n      try {\n        const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/chocolates');\n        this.chocolates = response.data;\n      } catch (error) {\n        console.error('Greška pri preuzimanju čokolada:', error);\n      }\n    },\n    getChocolatePicture(pictureName) {\n      return `/images/${pictureName}`;\n    },\n    editChocolate(id) {\n      this.$router.push({\n        name: 'editchocolate',\n        params: {\n          id\n        }\n      });\n    },\n    deleteChocolate(id) {\n      try {\n        axios.delete(`http://localhost:8080/WebShopAppREST/rest/chocolates/${id}`);\n        // Odmah ažuriraj prikaz nakon brisanja čokolade\n        this.chocolates = this.chocolates.filter(chocolate => chocolate.id !== id);\n      } catch (error) {\n        console.error('Greška pri brisanju čokolade:', error);\n      }\n    },\n    addChocolate() {\n      console.log('Add new chocolate');\n    }\n  }\n};","map":{"version":3,"names":["axios","useRouter","data","chocolates","created","fetchChocolates","methods","response","get","error","console","getChocolatePicture","pictureName","editChocolate","id","$router","push","name","params","deleteChocolate","delete","filter","chocolate","addChocolate","log"],"sources":["C:\\Users\\PC\\eclipse-workspace\\apache-tomcat-9.0.89\\apache-tomcat-9.0.89\\webapps\\chocolate-factory-2\\Kostur\\Frontend\\front-app\\src\\views\\AllChocolates.vue"],"sourcesContent":["<template>\r\n  <div class=\"all-chocolates\">\r\n    <table>\r\n      <thead>\r\n        <tr>\r\n          <th>Slika</th>\r\n          <th>Naziv</th>\r\n          <th>Cena</th>\r\n          <th>Tip</th>\r\n          <th>Vrsta</th>\r\n          <th>Težina</th>\r\n          <th>Opis</th>\r\n          <th>Fabrika</th>\r\n          <th>EDIT</th>\r\n          <th>DELETE</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"chocolate in chocolates\" :key=\"chocolate.id\">\r\n          <td><img :src=\"getChocolatePicture(chocolate.picture)\" :alt=\"chocolate.name\" class=\"chocolate-picture\" /></td>\r\n          <td>{{ chocolate.name }}</td>\r\n          <td>{{ chocolate.price }}</td>\r\n          <td>{{ chocolate.type }}</td>\r\n          <td>{{ chocolate.kind }}</td>\r\n          <td>{{ chocolate.weight }}</td>\r\n          <td>{{ chocolate.description }}</td>\r\n          <td>{{ chocolate.chocolateFactoryObject.name }}</td>\r\n          <td><button @click=\"editChocolate(chocolate.id)\">EDIT</button></td>\r\n          <td><button @click=\"deleteChocolate(chocolate.id)\">DELETE</button></td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n    <button @click=\"addChocolate\" class=\"add-chocolate-button\">ADD CHOCOLATE</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { useRouter } from 'vue-router';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      chocolates: []\r\n    };\r\n  },\r\n  async created() {\r\n    await this.fetchChocolates();\r\n  },\r\n  methods: {\r\n    async fetchChocolates() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/chocolates');\r\n        this.chocolates = response.data;\r\n      } catch (error) {\r\n        console.error('Greška pri preuzimanju čokolada:', error);\r\n      }\r\n    },\r\n    getChocolatePicture(pictureName) {\r\n      return `/images/${pictureName}`;\r\n    },\r\n    editChocolate(id) {\r\n      this.$router.push({ name: 'editchocolate', params: { id } });\r\n    },\r\n    deleteChocolate(id) {\r\n      try {\r\n    axios.delete(`http://localhost:8080/WebShopAppREST/rest/chocolates/${id}`);\r\n    // Odmah ažuriraj prikaz nakon brisanja čokolade\r\n    this.chocolates = this.chocolates.filter(chocolate => chocolate.id !== id);\r\n  } catch (error) {\r\n    console.error('Greška pri brisanju čokolade:', error);\r\n  }\r\n    },\r\n    addChocolate() {\r\n      console.log('Add new chocolate');\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.all-chocolates {\r\n  overflow-x: auto;\r\n}\r\n\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\n\r\nth, td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n  text-align: left;\r\n}\r\n\r\nth {\r\n  background-color: #f2f2f2;\r\n}\r\n\r\n.chocolate-picture {\r\n  width: 50px;\r\n  height: 50px;\r\n  object-fit: cover;\r\n}\r\n\r\n.add-chocolate-button {\r\n  margin-top: 20px;\r\n  margin-bottom: 20px;\r\n  margin-left: 0;\r\n}\r\n</style>\r\n"],"mappings":";AAqCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,YAAY;AAEtC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,eAAe,CAAC,CAAC;EAC9B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMP,KAAK,CAACQ,GAAG,CAAC,sDAAsD,CAAC;QACxF,IAAI,CAACL,UAAS,GAAII,QAAQ,CAACL,IAAI;MACjC,EAAE,OAAOO,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,kCAAkC,EAAEA,KAAK,CAAC;MAC1D;IACF,CAAC;IACDE,mBAAmBA,CAACC,WAAW,EAAE;MAC/B,OAAQ,WAAUA,WAAY,EAAC;IACjC,CAAC;IACDC,aAAaA,CAACC,EAAE,EAAE;MAChB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAE,eAAe;QAAEC,MAAM,EAAE;UAAEJ;QAAG;MAAE,CAAC,CAAC;IAC9D,CAAC;IACDK,eAAeA,CAACL,EAAE,EAAE;MAClB,IAAI;QACNd,KAAK,CAACoB,MAAM,CAAE,wDAAuDN,EAAG,EAAC,CAAC;QAC1E;QACA,IAAI,CAACX,UAAS,GAAI,IAAI,CAACA,UAAU,CAACkB,MAAM,CAACC,SAAQ,IAAKA,SAAS,CAACR,EAAC,KAAMA,EAAE,CAAC;MAC5E,EAAE,OAAOL,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,+BAA+B,EAAEA,KAAK,CAAC;MACvD;IACE,CAAC;IACDc,YAAYA,CAAA,EAAG;MACbb,OAAO,CAACc,GAAG,CAAC,mBAAmB,CAAC;IAClC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}