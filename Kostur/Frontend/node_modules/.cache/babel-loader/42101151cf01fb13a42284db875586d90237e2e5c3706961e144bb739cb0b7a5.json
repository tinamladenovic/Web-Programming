<option value=\"MANAGER\">MANAGER</option>\r\n        </select>\r\n      </div>\r\n\r\n      <button type=\"submit\">Create Manager</button>\r\n    </form>\r\n    <p v-if=\"errorMessage\" class=\"error\">{{ errorMessage }}</p>\r\n    <p v-if=\"successMessage\" class=\"success\">{{ successMessage }}</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      newUser: {\r\n        firstName: '',\r\n        lastName: '',\r\n        username: '',\r\n        password: '',\r\n        gender: 'MALE',\r\n        birthDate: '',\r\n        role: 'MANAGER',\r\n        customerType: 'NO_TYPE',\r\n        earnedPoints: 0\r\n      },\r\n      errorMessage: '',\r\n      successMessage: ''\r\n    };\r\n  },\r\n  \r\n  async created() {\r\n    // Proveri trenutnog korisnika\r\n    try {\r\n      const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/currentUser', { withCredentials: true });\r\n      const currentUser = response.data;\r\n      \r\n      if (currentUser.role !== 'ADMIN') {\r\n        this.$router.push('/unauthorized'); // Ili neka druga ruta gde preusmeravate korisnika\r\n      }\r\n    } catch (error) {\r\n      console.error('Error fetching current user:', error);\r\n      this.$router.push('/login'); // Ako nije ulogovan, preusmeri na login stranicu\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    async createUser() {\r\n      try {\r\n        const response = await axios.post('http://localhost:8080/WebShopAppREST/rest/users/createmanager', this.newUser);\r\n        this.successMessage = 'Manager created successfully!';\r\n        this.errorMessage = '';\r\n\r\n        this.clearForm();\r\n      } catch (error) {\r\n        this.successMessage = '';\r\n        if (error.response && error.response.data && error.response.data.message) {\r\n          this.errorMessage = error.response.data.message;\r\n        } else {\r\n          this.errorMessage = 'Failed to create manager.';\r\n        }\r\n      }\r\n    },\r\n    clearForm() {\r\n      this.newUser = {\r\n        firstName: '',\r\n        lastName: '',\r\n        username: '',\r\n        password: '',\r\n        gender: 'MALE',\r\n        birthDate: '',\r\n        role: 'MANAGER',\r\n        customerType: 'NO_TYPE',\r\n        earnedPoints: 0\r\n      };\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.add-manager {\r\n  max-width: 600px;\r\n  margin: 20px auto;\r\n  padding: 20px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 8px;\r\n  background-color: #f9f9f9;\r\n}\r\n\r\nh2 {\r\n  text-align: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 15px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  margin-bottom: 5px;\r\n  font-weight: bold;\r\n}\r\n\r\ninput[type=\"text\"],\r\ninput[type=\"password\"],\r\ninput[type=\"date\"],\r\nselect {\r\n  width: 100%;\r\n  padding: 8px;\r\n  font-size: 16px;\r\n  margin-bottom: 5px;\r\n}\r\n\r\nbutton {\r\n  width: 100%;\r\n  padding: 10px 20px;\r\n  background-color: #4CAF50;\r\n  color: #fff;\r\n  border: none;\r\n  cursor: pointer;\r\n  font-size: 16px;\r\n  border-radius: 4px;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #45a049;\r\n}\r\n\r\n.error {\r\n  color: red;\r\n  margin-top: 10px;\r\n}\r\n\r\n.success {\r\n  color: green;\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"],"mappings":";AAoDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE,MAAM;QACdC,SAAS,EAAE,EAAE;QACbC,IAAI,EAAE,SAAS;QACfC,YAAY,EAAE,SAAS;QACvBC,YAAY,EAAE;MAChB,CAAC;MACDC,YAAY,EAAE,EAAE;MAChBC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EAED,MAAMC,OAAOA,CAAA,EAAG;IACd;IACA,IAAI;MACF,MAAMC,QAAO,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAAC,uDAAuD,EAAE;QAAEC,eAAe,EAAE;MAAK,CAAC,CAAC;MACpH,MAAMC,WAAU,GAAIH,QAAQ,CAACd,IAAI;MAEjC,IAAIiB,WAAW,CAACT,IAAG,KAAM,OAAO,EAAE;QAChC,IAAI,CAACU,OAAO,CAACC,IAAI,CAAC,eAAe,CAAC,EAAE;