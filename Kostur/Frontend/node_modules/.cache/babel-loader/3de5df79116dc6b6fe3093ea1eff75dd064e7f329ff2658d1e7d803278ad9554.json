actory: null,\r\n    };\r\n  },\r\n  methods: {\r\n    loadFactory() {\r\n      axios.get(`http://localhost:8080/WebShopAppREST/rest/chocolateFactoryObjects/${this.factoryId}`)\r\n        .then(response => {\r\n          this.factory = response.data;\r\n          console.log('Podaci o fabrici:', this.factory); // Dodajte ovaj red za proveru\r\n        })\r\n        .catch(error => {\r\n          console.error(\"Greška pri učitavanju fabrike:\", error);\r\n        });\r\n    },\r\n    getFactoryLogo(logoFileName) {\r\n      return `/images/${logoFileName}`;\r\n    },\r\n  },\r\n  mounted() {\r\n    this.loadFactory();\r\n  },\r\n};\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.factory-profile {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  padding: 20px;\r\n}\r\n\r\n.factory-info {\r\n  flex: 1;\r\n  margin-right: 20px;\r\n}\r\n\r\n.factory-logo {\r\n  width: 100px;\r\n  height: 100px;\r\n}\r\n</style>\r\n"],"mappings":"AAcA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,OAAOC,KAAI,MAAO,OAAO;;AAEzB;AACA,eAAe;EACbC,KAAK,EAAE;IACLC,SAAS,EAAEC;EACb,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZP,KAAK,CAACQ,GAAG,CAAE,qEAAoE,IAAI,CAACN,SAAU,EAAC,EAC5FO,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACL,OAAM,GAAIK,QAAQ,CAACN,IAAI;QAC5BO,OAAO,CAACC,GAAG,CAAC,mBAAmB,EAAE,IAAI,CAACP,OAAO,CAAC,EAAE;MAClD,CAAC,EACAQ,KAAK,CAACC,KAAI,IAAK;QACdH,OAAO,CAACG,KAAK,CAAC,gCAAgC,EAAEA,KAAK,CAAC;MACxD,CAAC,CAAC;IACN,CAAC;IACDC,cAAcA,CAACC,YAAY,EAAE;MAC3B,OAAQ,WAAUA,YAAa,EAAC;IAClC;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACV,WAAW,CAAC,CAAC;EACpB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}