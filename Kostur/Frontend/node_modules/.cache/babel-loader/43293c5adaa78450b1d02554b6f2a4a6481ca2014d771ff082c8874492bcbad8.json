{"ast":null,"code":"import axios from 'axios';\nimport { ref, onMounted, computed } from 'vue';\nexport default {\n  setup() {\n    const users = ref([]);\n    const searchQuery = ref('');\n    const sortBy = ref('firstName');\n    const roleFilter = ref('');\n    const fetchUsers = async () => {\n      try {\n        const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/users', {\n          withCredentials: true\n        });\n        users.value = response.data;\n        console.log('Fetched users:', users.value); // Log after successful fetch\n      } catch (error) {\n        console.error('Error fetching users:', error);\n      }\n    };\n    onMounted(() => {\n      fetchUsers();\n    });\n    const filteredUsers = computed(() => {\n      let filtered = users.value;\n\n      // Filter by search query\n      if (searchQuery.value.trim()) {\n        const lowerCaseQuery = searchQuery.value.toLowerCase();\n        filtered = filtered.filter(user => user.firstName.toLowerCase().includes(lowerCaseQuery) || user.lastName.toLowerCase().includes(lowerCaseQuery) || user.username.toLowerCase().includes(lowerCaseQuery));\n      }\n\n      // Filter by role if roleFilter is set\n      if (roleFilter.value) {\n        filtered = filtered.filter(user => user.role === roleFilter.value);\n      }\n\n      // Sort users based on selected sortBy value\n      if (sortBy.value.startsWith('-')) {\n        const key = sortBy.value.substring(1);\n        filtered.sort((a, b) => b[key].localeCompare(a[key])); // Descending sort\n      } else {\n        filtered.sort((a, b) => a[sortBy.value].localeCompare(b[sortBy.value])); // Ascending sort\n      }\n      return filtered;\n    });\n    return {\n      searchQuery,\n      filteredUsers,\n      sortBy,\n      roleFilter\n    };\n  }\n};","map":{"version":3,"names":["axios","ref","onMounted","computed","setup","users","searchQuery","sortBy","roleFilter","fetchUsers","response","get","withCredentials","value","data"