    </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"user in filteredUsers\" :key=\"user.id\">\r\n          <td>{{ user.firstName }}</td>\r\n          <td>{{ user.lastName }}</td>\r\n          <td>{{ user.username }}</td>\r\n          <td>{{ user.earnedPoints }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { ref, onMounted, computed } from 'vue';\r\n\r\nexport default {\r\n  setup() {\r\n    const users = ref([]);\r\n    const searchQuery = ref('');\r\n    const sortBy = ref('firstName');\r\n    const roleFilter = ref('');\r\n\r\n    const fetchUsers = async () => {\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/WebShopAppREST/rest/users', {\r\n          withCredentials: true,\r\n        });\r\n        users.value = response.data;\r\n        console.log('Fetched users:', users.value); // Log after successful fetch\r\n      } catch (error) {\r\n        console.error('Error fetching users:', error);\r\n      }\r\n    };\r\n\r\n    onMounted(() => {\r\n      fetchUsers();\r\n    });\r\n\r\n    const filteredUsers = computed(() => {\r\n      let filtered = users.value;\r\n\r\n      // Filter by search query\r\n      if (searchQuery.value.trim()) {\r\n        const lowerCaseQuery = searchQuery.value.toLowerCase();\r\n        filtered = filtered.filter(user =>\r\n          user.firstName.toLowerCase().includes(lowerCaseQuery) ||\r\n          user.lastName.toLowerCase().includes(lowerCaseQuery) ||\r\n          user.username.toLowerCase().includes(lowerCaseQuery)\r\n        );\r\n      }\r\n\r\n      // Filter by role if roleFilter is set\r\n      if (roleFilter.value) {\r\n        filtered = filtered.filter(user => user.role === roleFilter.value);\r\n      }\r\n\r\n      // Sort users based on selected sortBy value\r\n      if (sortBy.value.startsWith('-')) {\r\n        const key = sortBy.value.substring(1);\r\n        filtered.sort((a, b) => b[key].localeCompare(a[key])); // Descending sort\r\n      } else {\r\n        filtered.sort((a, b) => a[sortBy.value].localeCompare(b[sortBy.value])); // Ascending sort\r\n      }\r\n\r\n      return filtered;\r\n    });\r\n\r\n    return {\r\n      searchQuery,\r\n      filteredUsers,\r\n      sortBy,\r\n      roleFilter,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\n\r\n.table th, .table td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n}\r\n\r\n.table th {\r\n  background-color: #f2f2f2;\r\n  text-align: left;\r\n}\r\n</style>\r\n"],"mappings":"AA2DA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,GAAG,EAAEC,SAAS,EAAEC,QAAO,QAAS,KAAK;AAE9C,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIJ,GAAG,CAAC,EAAE,CAAC;IACrB,MAAMK,WAAU,GAAIL,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMM,MAAK,GAAIN,GAAG,CAAC,WAAW,CAAC;IAC/B,MAAMO,UAAS,GAAIP,GAAG,CAAC,EAAE,CAAC;IAE1B,MAAMQ,UAAS,GAAI,MAAAA,CAAA,KAAY;MAC7B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMV,KAAK,CAACW,GAAG,CAAC,iDAAiD,EAAE;UAClFC,eAAe,EAAE;QACnB,CAAC,CAAC;QACFP,KAAK,CAACQ,KAAI,GAAIH,QAAQ,CAACI,IAAI;QAC3BC,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEX,KAAK,CAACQ,KAAK,CAAC,EAAE;MAC9C,EAAE,OAAOI,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C;IACF,CAAC;IAEDf,SAAS,CAAC,MAAM;MACdO,UAAU,CAAC,CAAC;IACd,CAAC,CAAC;IAEF,MAAMS,aAAY,GAAIf,QAAQ,CAAC,MAAM;MACnC,IAAIgB,QAAO,GAAId,KAAK,CAACQ,KAAK;;MAE1B;MACA,IAAIP,WAAW,CAACO,KAAK,CAACO,IAAI,CAAC,CAAC,EAAE;QAC5B,MAAMC,cAAa,GAAIf,WAAW,CAACO,KAAK,CAACS,WAAW,CAAC,CAAC;QACtDH,QAAO,GAAIA,QAAQ,CAACI,MAAM,CAACC,IAAG,IAC5BA,IAAI,CAACC,SAAS,CAACH,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACL,cAAc,KACpDG,IAAI,CAACG,QAAQ,CAACL,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACL,cAAc,KACnDG,IAAI,CAACI,QAAQ,CAACN,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACL,cAAc,CACrD,CAAC;MACH;;MAEA;MACA,IAAIb,UAAU,CAACK,KAAK,EAAE;QACpBM,QAAO,GAAIA,QAAQ,CAACI,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACK,IAAG,KAAMrB,UAAU,CAACK,KAAK,CAAC;MACpE;;MAEA;MACA,IAAIN,MAAM,CAACM,KAAK,CAACiB,UAAU,CAAC,GAAG,CAAC,EAAE;Q